<p id="notice"><%= notice %></p>

<h1>Listing Receipts</h1>

<table>
  <thead>
    <tr>
      <th>Cart item</th>
      <th>City</th>
      <th>Adr 1</th>
      <th>Adr 2</th>
      <th>State</th>
      <th>Zip</th>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receipts.each do |receipt| %>
      <tr>
        <td><%= receipt.cart_item_id %></td>
        <td><%= receipt.city %></td>
        <td><%= receipt.adr_1 %></td>
        <td><%= receipt.adr_2 %></td>
        <td><%= receipt.state %></td>
        <td><%= receipt.zip %></td>
        <td><%= receipt.name %></td>
        <td><%= receipt.price %></td>
        <td><%= link_to 'Show', receipt %></td>
        <td><%= link_to 'Edit', edit_receipt_path(receipt) %></td>
        <td><%= link_to 'Destroy', receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receipt', new_receipt_path %>
